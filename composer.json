{
    "name": "mikk150/yii2-queuemailer",
    "description": "yii2 queue mailer component",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","queue", "mailer"],
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Mikk Tendermann",
            "email": "mikk150@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.0",
        "yiisoft/yii2-queue": "2.2.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.4",
        "codeception/codeception": "~2.2.11",
        "codeception/specify": "*",
        "codeception/verify": "*",
        "yiisoft/yii2-swiftmailer": "*"
    },
    "autoload": {
        "psr-4": {
            "mikk150\\queuemailer\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://gitlab.com/yii2-queue/yii2-queue"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
